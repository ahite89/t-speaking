<div class="row">
  <div class="large-3 columns ">
    <div>
      <%= image_tag("clef.png") %>
      <h5></h5>
    </div>
  </div>

  <div class="large-6 columns">
    <% @questions.each do |question| %>
      <div class="large-2 columns small-3"><%= image_tag("bass.png") %></div>
        <div class="large-10 columns" id="questions">
          <%= raw question.tag_list.map { |t| link_to t, tag_path(t) }.join(', ') %>
          <p><strong><%= link_to question.title, question_path(question) %></strong></p>
          <ul class="inline-list">
            <li><%= link_to "edit", edit_question_path(question) %></li>
            <li><%= link_to "delete", question_path(question), method: :delete %></li>
          </ul>
        </div>
        <hr/>
      <% end %>
    </div>
    <hr/>
  </div>
</div>
